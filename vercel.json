{
    "name":"venta-productos",
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "uploads/**/*",
            "use": "@vercel/static"
        }      
    ],
    
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        },
        {
            "src": "/uploads/(.*)",
            "dest": "/uploads/$1"
        }
    ]
}